{
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "react",
  "editor.fontFamily": "Jetbrains Mono, Input, Menlo, Monaco, 'Courier New', monospace",
  "editor.tokenColorCustomizations": {
    // Italicize comments
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "fontStyle": "italic"
        }
      }
    ]
  },
  "gitlens.currentLine.enabled": false,
  "workbench.colorTheme": "GitHub Dark",
  "vsicons.dontShowNewVersionMessage": true,
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "editor.lineHeight": 16,
  "editor.fontSize": 13,
  "editor.formatOnPaste": true,
  "customizeUI.font.regular": "Jetbrains Mono",
  "customizeUI.font.monospace": "Jetbrains Mono",
  "workbench.preferredDarkColorTheme": "Default Light+",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.find.seedSearchStringFromSelection": false,
  "go.toolsManagement.autoUpdate": true,
  "security.workspace.trust.untrustedFiles": "open"
}
